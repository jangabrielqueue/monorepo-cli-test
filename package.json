{
  "name": "default",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@microsoft/signalr": "^3.1.0",
    "antd": "^3.26.7",
    "axios": "^0.19.0",
    "dayjs": "^1.8.17",
    "env-cmd": "^10.0.1",
    "eslint-config-prettier": "^6.1.0",
    "firebase": "^7.7.0",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-app-polyfill": "^1.0.2",
    "react-dom": "^16.9.0",
    "react-error-boundary": "^1.2.5",
    "react-intl": "^3.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "source-map-explorer": "^2.0.1"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy:uat": "env-cmd -f .env.uat react-scripts build && firebase use default && firebase deploy --only hosting:topup-uat-ezpay",
    "deploy:prod": "env-cmd -f .env.production react-scripts build && firebase use prod && firebase deploy --only hosting:deposit-prod-ezpay",
    "deploy:dev": "env-cmd -f .env.development react-scripts build && firebase use default && firebase deploy --only hosting:deposit-dev-ezpay",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.3.0"
  }
}
