{
  "name": "default",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@microsoft/signalr": "^3.0.0",
    "antd": "^3.23.1",
    "axios": "^0.19.0",
    "dayjs": "^1.8.17",
    "env-cmd": "^10.0.1",
    "eslint-config-prettier": "^6.1.0",
    "node-sass": "^4.12.0",
